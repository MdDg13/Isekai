{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "build:turbo": "next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "test": "vitest run",
    "test:ui": "vitest",
    "e2e": "playwright test",
    "pre-pr": "eslint && npx tsc --noEmit && vitest run",
    "gen:types": "powershell -ExecutionPolicy Bypass -Command \"if (!(Test-Path src/types)) { New-Item -ItemType Directory -Path src/types -Force | Out-Null }; if (Get-Command supabase -ErrorAction SilentlyContinue) { supabase gen types typescript --project-id $env:SUPABASE_PROJECT_ID --schema public | Out-File -FilePath src/types/supabase.ts -Encoding UTF8 } else { Write-Host 'supabase CLI not found; skipping types generation' }\"",
    "auto-fix": "powershell -ExecutionPolicy Bypass -File scripts/auto-fix-cycle.ps1",
    "check-logs": "powershell -ExecutionPolicy Bypass -File scripts/check-logs.ps1",
    "analyze-npcs": "powershell -ExecutionPolicy Bypass -File scripts/analyze-npcs.ps1",
    "cf:logs": "powershell -ExecutionPolicy Bypass -File scripts/fetch-cf-logs.ps1",
    "extract-free5e": "powershell -ExecutionPolicy Bypass -File scripts/extract-free5e-data.ps1",
    "parse-free5e": "tsx scripts/parse-free5e-markdown.ts",
    "parse-all-content": "tsx scripts/parse-all-content.ts",
    "extract-all-content": "tsx scripts/extract-all-content.ts",
    "extract-enhanced": "tsx scripts/extract-enhanced-content.ts",
    "extract-pdfs": "tsx scripts/extract-from-pdfs.ts",
    "qc-review": "tsx scripts/qc-review-extracted-data.ts",
    "qc-analyze": "tsx scripts/collect-qc-feedback.ts",
    "qc-retrieve": "powershell -ExecutionPolicy Bypass -File scripts/retrieve-qc-feedback.ps1",
    "update-existing-data": "tsx scripts/update-existing-data.ts",
    "validate-extracted": "tsx scripts/validate-extracted-data.ts",
    "sample-data": "tsx scripts/sample-and-validate.ts",
    "generate:ai": "node scripts/generate-ai-assets.mjs"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.75.0",
    "cytoscape": "^3.33.1",
    "jspdf": "^3.0.3",
    "next": "15.5.4",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "vis-timeline": "^8.3.1"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20251014.0",
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.56.1",
    "@tailwindcss/postcss": "^4.1.16",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20",
    "@types/pdf-parse": "^1.1.5",
    "@types/react": "19.2.2",
    "@types/react-dom": "^19",
    "@types/testing-library__jest-dom": "^5.14.9",
    "eslint": "^9",
    "eslint-config-next": "15.5.4",
    "jsdom": "^27.0.1",
    "pdf-parse": "^2.4.5",
    "tailwindcss": "^4",
    "tsx": "^4.20.6",
    "typescript": "5.9.3",
    "vitest": "^4.0.5"
  }
}
